<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<input type='text' placeholder='search' id='search'>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render partial: 'user', collection: @users %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path, remote: true, class: 'new_user_link' %>
